{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "max-len": [
      "error",
      180,
      4,
      {
        "ignoreUrls": true
      }
    ],
    "one-var": [
      "error",
      "never"
    ],
    "no-param-reassign": [
      "error",
      {
        "props": false
      }
    ],
    "no-underscore-dangle": 0,
    "object-shorthand": [
      2,
      "properties"
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "strict": [
      0,
      "global"
    ],
    "comma-dangle": [
      "error",
      "never"
    ],
    "quote-props": [
      "error",
      "consistent"
    ]
  }
}
